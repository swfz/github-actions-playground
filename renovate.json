{
  "labels": ["renovate"],
  "regexManagers": [
    {
      "fileMatch": ["hoge.yml"],
      "matchStrings": ["uses: (?<depName>.*?)@(?<currentValue>.*?)$"],
      "datasourceTemplate": "github-tags"
    },
    {
      "fileMatch": ["sample.yml"],
      "matchStrings": ["version: (?<depName>.*?)@(?<currentValue>.*?)\n"],
      "datasourceTemplate": "github-tags"
    }
  ]
}
