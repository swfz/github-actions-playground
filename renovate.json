{
  "extends": [
    "config:base"
  ],
  "labels": ["renovate"],
  "github-actions": {
    "fileMatch": [".github/workflows/blank.yml",".github/workflows/takeover-output.yml"]
  },
  "regexManagers": [
    {
      "fileMatch": ["sample.yml"],
      "matchStrings": ["version: (?<depName>.*?)@(?<currentValue>.*?)\n"],
      "datasourceTemplate": "github-tags"
    }
  ]
}
